<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.controllers.WatchOptionsController">
   <children>
      <HBox alignment="CENTER_RIGHT" spacing="10.0">
         <children>
            <Label text="Watch ID: " />
            <Label fx:id="labelWatchID" prefHeight="17.0" prefWidth="149.0" text="WATCH ID" />
         </children>
      </HBox>
      <HBox alignment="CENTER_RIGHT" spacing="10.0">
         <children>
            <Label text="Watch name: " />
            <TextField fx:id="textfieldWatchName" />
         </children>
      </HBox>
      <Separator prefWidth="200.0" />
      <Button mnemonicParsing="false" onAction="#saveOptions" text="Save" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
